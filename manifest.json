{
    "manifest_version": 3,
    "name": "Cache Accessor",
    "description": "A Chrome extension to access and view Cache Storage data",
    "version": "1.0",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "cacheStorage",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "data:image/png;base64,"
        }
    }
}